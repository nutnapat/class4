<%= form_with(model: @user) do |form| %>
  
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :pass %>
    <%= form.password_field :password %>
  </div>

   <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
